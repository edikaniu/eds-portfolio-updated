{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "dev:analyze": "ANALYZE=true next dev",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:reset": "rm -rf .next node_modules package-lock.json && npm install --legacy-peer-deps && npm run dev",
    "dev:stable": "rm -rf .next/.cache && next dev",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit --skipLibCheck",
    "clean": "rm -rf .next node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install --legacy-peer-deps",
    "predev": "npm run type-check",
    "cache:clear": "rm -rf .next/.cache .next/cache",
    "init-admin": "npx tsx scripts/init-admin.ts",
    "db:reset": "npx prisma migrate reset --force && npm run init-admin",
    "analyze:bundle": "npx next-bundle-analyzer",
    "analyze:size": "npm run build && npx size-limit",
    "optimize": "npm run build:analyze && npm run analyze:size",
    "postinstall": "prisma generate",
    "deploy:vercel": "node scripts/deploy-vercel.js",
    "deploy:vercel:prod": "node scripts/deploy-vercel.js --prod",
    "deploy:vercel:preview": "node scripts/deploy-vercel.js --preview",
    "vercel:build": "npm run build",
    "vercel:dev": "vercel dev",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "validate:deployment": "node scripts/validate-deployment.js",
    "test:endpoints": "node scripts/test-endpoints.js",
    "test:endpoints:prod": "node scripts/test-endpoints.js https://your-app.vercel.app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:setup": "npx tsx scripts/test-setup.ts setup",
    "test:cleanup": "npx tsx scripts/test-setup.ts cleanup",
    "test:seed": "npx tsx scripts/test-setup.ts seed",
    "test:all": "npm run test:setup && npm run test && npm run test:e2e && npm run test:cleanup",
    "test:ci": "npm run test:setup && npm run test:coverage && npm run test:e2e --reporter=junit && npm run test:cleanup"
  },
